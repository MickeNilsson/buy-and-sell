{
  "name": "buyandsell",
  "version": "1.0.0",
  "description": "Buy and sell site",
  "main": "./js/index.js",
  "scripts": {
    "build": "npm run minifycss && npm run minifyjs && npm run bundle",
    "bundle": "concat -o ./dist/index.html ./html/indextop.html ./css/bootstrap.min.css ./css/index.min.css ./html/indexmiddle.html ./js/jquery.min.js ./js/bootstrap.bundle.min.js ./js/index.min.js ./html/indexbottom.html",
    "minifycss": "minify ./css/index.css > ./css/index.min.css",
    "minifyjs": "minify ./js/index.js > ./js/index.min.js",
    "server": "http-server -a localhost -p 8000 -c-1"
  },
  "keywords": [
    "buy",
    "sell"
  ],
  "author": "Mikael Nilsson",
  "license": "ISC",
  "devDependencies": {
    "concat": "^1.0.3",
    "eslint": "^5.6.1",
    "http-server": "^0.11.1",
    "minify": "^3.0.5"
  },
  "dependencies": {
    "normalize.css": "^8.0.0"
  }
}
